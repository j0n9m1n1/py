$(function() {

  $(".ibox3").click(function() {

    $(".ibox3").removeClass("wb-choice");

    $(this).addClass("ibox3");

    

    document.location.href = "/ilos/st/course/online_list_form.acl?WEEK_NO="+$(this).find(".wb-week").attr("var");

    

