<script>

          var start1 = -1;

          $(function() {

            newCmmtList();

          });  

          function newCmmtList(){

            start1++;

           

            $.ajax({

                url: "/ilos/st/course/submain_newcmmt_list.acl",

                type: "POST",

                data: { 

                   start :  (start1*10)+1, 

                   display : "10",  

                   encoding : "utf-8"

                      },

                async: false,

                success: function(html){

                     $("#newCmmtList_div").append(html);

                  },

                error : function(html) {

                     //alert("error:"+html);

                  }

              });

          }

          </script>
<div class="submain-right" style="padding: 0;">
<div class="title">최근댓글</div>
<div class="submain-noticebox" style="padding: 9px; margin: 0;">
<div id="newCmmtList_div"></div>
<div id="newCmmtAddBtn" style="margin-top: 5px; text-align: center;">
<img alt="더보기" onclick="newCmmtList();" src="/ilos/images/common/add_view.png" style="cursor: pointer;"/>
</div>
</div>
