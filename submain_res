  var last_cnt = $(".submain-notice").length;

  $(".submain-notice").each(function(index) {

    if(index == 0){

      $(this).addClass("submain-notice-first");

    }

    if(index == Number(last_cnt) -1){

      $(this).addClass("submain-notice-last");

    }

  });

});

