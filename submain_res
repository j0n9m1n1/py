<script>

          var start = -1;

          $(function() {

            newRegList();

          });  

          function newRegList(){

            start++;

           

            $.ajax({

                url: "/ilos/st/course/submain_newreg_list.acl",

                type: "POST",

                data: { 

                   start :  (start*10)+1, 

                   display : "10",  

                   encoding : "utf-8"

                      },

                async: false,

                success: function(html){

                     $("#newRegList_div").append(html);

                  },

                error : function(html) {

                     //alert("error:"+html);

                  }

              });

          }

          </script>
